vegetables <- c("Vegetables")

fruits <- c("Fruits - Excluding Wine") 

alko <- c("Alcoholic Beverages") # bez podziału też może być ciekawe

spices <- c('Spices')

meat <- c('Meat') # też z podziałem na rodzaj mięsa

